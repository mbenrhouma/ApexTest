<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" access="global" controller="CEL_AccountDetailsController">
    <aura:attribute name="recordError" type="String" />
    <aura:attribute name="recordInfo" type="Object" />
    <aura:attribute name="simpleRecord" type="Object" />
    <aura:attribute name="fieldAccess" type="Map" access="global" />
    <aura:attribute name="isEditMode" type="Boolean" default="false" />
    <aura:attribute name="allCountries" type="List" access="global" />
    <aura:attribute name="allLanguages" type="List" access="global" />
    <aura:attribute name="allChannels" type="List" access="global" />
    <aura:attribute name="allAppointment" type="List" access="global" />
    <aura:attribute name="isExpandedSectionOne" type="Boolean" default="true" />
    <aura:attribute name="isExpandedSectionTwo" type="Boolean" default="true" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="newFormError" type="String" />

    <div class="{! 'slds-section' + (v.isExpandedSectionOne ? ' slds-is-open' : '')}">
        <h3 class="slds-section__title">
            <button aria-controls="expando-unique-id" class="slds-button slds-section__title-action" aria-expanded="{!v.isExpandedSectionOne}" onclick="{!c.changeStateSectionOne}">
                <lightning:icon class="icn_section" iconName="{!v.isExpandedSectionOne ? 'utility:chevrondown' : 'utility:chevronright'}" size="xx-small" />
                <span class="slds-truncate" title="Section Title">{!$Label.c.cel_contact_details}</span>
            </button>
        </h3>
        <div aria-hidden="{!!v.isExpandedSectionOne}" class="slds-section__content" id="expando-unique-id">
            <section class="tabs_content active uiTab" aria-expanded="true" role="tabpanel" data-aura-class="uiTab">
                <div class="flexipageComponent" data-aura-class="flexipageComponent">
                    <div class="slds-clearfix slds-card groupDependantFieldEnabled allow-horizontal-form forceDetail"></div>
                </div>
            </section>
            <aura:if isTrue="{!v.isEditMode}">
                <force:recordData aura:id="recordHandler" recordId="{!v.recordId}" layoutType="FULL" targetRecord="{!v.recordInfo}" targetFields="{!v.simpleRecord}" mode="EDIT" fields="Id,cel_PersonEmail__c,
                        cel_email_2__c,
                        cel_optin_postal_mail__c,
                        PersonHomePhone,
                        PersonMobilePhone,
                        cel_wechat_id__c,
                        PersonOtherPhone,
                        PersonDoNotCall,
                        cel_email_validity__c,
                        cel_homephone_validity__c,
                        cel_mobile_validity__c,
                        cel_optin_sms__c,
                        cel_do_not_email__c,
                        cel_otherphone_validity__c,
                        cel_addr_1_validity__c,
                        cel_line_id__c,
                        cel_addr_1_line_1__c,
                        cel_addr_1_line_2__c,
                        cel_addr_1_line_3__c,
                        cel_addr_1_zipcode__c,
                        cel_addr_1_region__c,
                        cel_addr_1_city__c,
                        cel_addr_1_country__c,
                        cel_preferred_contact_method__c,
                        cel_communication_language__c,
                        cel_preferred_appointment_time__c,
                        PersonEmail" />
