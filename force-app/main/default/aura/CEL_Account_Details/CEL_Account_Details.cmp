                                                <span class="test-id__field-value slds-form-element__static slds-grow">
                                                    <span class="uiOutputText">
                                                        <aura:if isTrue="{!not(empty(v.simpleRecord.cel_PersonEmail__c))}">                                                      
                                                            <aura:if isTrue="{!v.fieldAccess.emailValidity}">
                                                                <span class="slds-radio">
                                                                    <aura:if isTrue="{!v.simpleRecord.cel_email_validity__c}">
                                                                        <input type="radio" id="TrueValidityPhone" name="optionsValidityEmail" value="TrueValidityPhone" checked="checked" onchange="{!c.handleRatioState}" />
                                                                        <aura:set attribute="else">
                                                                            <input type="radio" id="TrueValidityPhone" name="optionsValidityEmail" value="TrueValidityPhone" onchange="{!c.handleRatioState}" />
                                                                        </aura:set>
                                                                    </aura:if> <label class="slds-radio__label" for="TrueValidityPhone">
                                                                        <span class="slds-radio_faux"></span> <span class="slds-form-element__label">
                                                                            <lightning:icon class="icn_green" iconName="utility:check" size="small" /></span>
                                                                    </label>
                                                                </span>
                                                                <span class="slds-radio">
                                                                    <aura:if isTrue="{!v.simpleRecord.cel_email_validity__c}">
                                                                        <input type="radio" id="FalseValidityPhone" name="optionsValidityEmail" value="FalseValidityPhone" onchange="{!c.handleRatioState}" />
                                                                        <aura:set attribute="else">
                                                                            <input type="radio" id="FalseValidityPhone" name="optionsValidityEmail" value="FalseValidityPhone" checked="checked" onchange="{!c.handleRatioState}" />
                                                                        </aura:set>
                                                                    </aura:if> <label class="slds-radio__label" for="FalseValidityPhone">
                                                                        <span class="slds-radio_faux"></span> <span class="slds-form-element__label">
                                                                            <lightning:icon class="icn_red" iconName="utility:close" size="small" /></span>
                                                                    </label>
                                                                </span>
                                                                <aura:set attribute="else">
                                                                    <aura:if isTrue="{!v.simpleRecord.emailValidity}">
                                                                        <lightning:icon class="icn_green" iconName="utility:check" size="small" />
