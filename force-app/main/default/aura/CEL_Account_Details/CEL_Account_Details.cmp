                                                                            <lightning:icon class="icn_red" iconName="utility:close" size="small" />
                                                                        </aura:set>
                                                                    </aura:if>
                                                                </aura:set>
                                                            </aura:if>
                                                        </aura:if>
                                                    </span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
